<Defaults>
  <Text fontSize="20" fontStyle="Semibold" color="#FFFFFF"/>
  <Toggle textColor="White"/>
</Defaults>
<Panel id="SetupPanel"
  color="#111111"
  outline="#FFFFFF"
  outlineSize="2 -2"
  Padding="20 20 16 16"
width="770"
height="990"
active="false"
rectAlignment="Middle"
showAnimation="Grow"
hideAnimation="Shrink"
returnToOriginalPositionWhenReleased="false"
allowDragging="true">
<VerticalLayout>
<Text fontSize="30">Setup</Text>
<VerticalLayout padding="8 8 8 0">
  <Text>Expansions</Text>
  <HorizontalLayout padding="20 0 4 8">
    <Toggle id="EH01" isOn="true" interactable="false">Base</Toggle>
    <Toggle id="EH02" isOn="true"
      onValueChanged="expansionValueChanged">FL</Toggle>
    <Toggle id="EH03" isOn="true"
      onValueChanged="expansionValueChanged">MoM</Toggle>
    <Toggle id="EH04" isOn="true"
      onValueChanged="expansionValueChanged">SR</Toggle>
    <Toggle id="EH05" isOn="true"
      onValueChanged="expansionValueChanged">UtP</Toggle>
    <Toggle id="EH06" isOn="true"
      onValueChanged="expansionValueChanged">SoC</Toggle>
    <Toggle id="EH07" isOn="true"
      onValueChanged="expansionValueChanged">TD</Toggle>
    <Toggle id="EH08" isOn="true"
      onValueChanged="expansionValueChanged">CiR</Toggle>
    <Toggle id="EH09" isOn="true"
      onValueChanged="expansionValueChanged">MoN</Toggle>
  </HorizontalLayout>
</VerticalLayout>
<VerticalLayout padding="8 8 8 0">
  <Text>Mythos Difficulty</Text>
  <HorizontalLayout padding="140 0 4 8">
    <Toggle id="easyMythosToggle" isOn="true"
      onValueChanged="easyMythosToggleValueChanged">Easy</Toggle>
    <Toggle id="mediumMythosToggle" isOn="true"
      onValueChanged="mediumMythosToggleValueChanged">Medium</Toggle>
    <Toggle id="hardMythosToggle" isOn="true"
      onValueChanged="hardMythosToggleValueChanged">Hard</Toggle>
  </HorizontalLayout>
</VerticalLayout>
  <VerticalLayout padding="8 0 0 8">
  <Text>Random Prelude</Text>
  <HorizontalLayout padding="330 0">
  <Toggle id="preludeToggle" isOn="false" onValueChanged="preludeToggleValueChanged">Yes</Toggle>
</HorizontalLayout>
</VerticalLayout>
  <Text>Official Ancient Ones</Text>
